{
  "expo": {
    "name": "Ben's example",
    "slug": "my-expo-project",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bugsnag.bentest"
    },
    "android": {
      "package": "com.bugsnag.bentest"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "./scratch/post-publish-hook",
          "config": {
            "apiKey": "API_KEY"
          }
        }
      ]
    }
  }
}
