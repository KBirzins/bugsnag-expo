{
  "name": "@bugsnag/expo",
  "version": "7.0.0-pre-alpha-ben.2",
  "main": "src/notifier.js",
  "types": "dist/types/bugsnag.d.ts",
  "description": "Bugsnag error reporter for Expo applications",
  "keywords": [
    "bugsnag",
    "expo",
    "react native",
    "exception",
    "error",
    "javascript",
    "monitoring",
    "reporting",
    "stability",
    "mobile"
  ],
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "hooks",
    "dist",
    "bin"
  ],
  "scripts": {
    "clean": "rm -fr dist && mkdir dist",
    "bundle-types": "../../bin/bundle-types",
    "build": "npm run clean && npm run bundle-types",
    "test": "nyc --reporter=lcov -x bin/lib/test/lib/* -x **/*.test.js -- jasmine '!(node_modules|types)/**/*.test.js'",
    "test:types": "tslint -c ../../tslint.json types/*.d.ts && jasmine 'types/**/*.test.js'",
    "postversion": "npm run build"
  },
  "bin": {
    "bugsnag-expo": "bin/cli.js"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "jasmine": "3.1.0",
    "ncp": "^2.0.0",
    "nyc": "^12.0.2",
    "proxyquire": "^2.1.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@bugsnag/core": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/delivery-expo": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-browser-session": "^6.0.0",
    "@bugsnag/plugin-console-breadcrumbs": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-expo-app": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-expo-device": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-network-breadcrumbs": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-react": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-react-native-app-state-breadcrumbs": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-react-native-connectivity-breadcrumbs": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-react-native-global-error-handler": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-react-native-orientation-breadcrumbs": "^7.0.0-pre-alpha-ben.2",
    "@bugsnag/plugin-react-native-unhandled-rejection": "^7.0.0-pre-alpha-ben.2",
    "bugsnag-build-reporter": "^1.0.1",
    "bugsnag-sourcemaps": "^1.1.0",
    "command-line-args": "^5.0.2",
    "kleur": "^3.0.2",
    "prompts": "^2.0.4"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*"
  }
